<script>
    $(document).ready(function () {
        var path = window.location.pathname;
        var path1 = window.location.href;

        path = path.replace(/\/$/, "");
        path = decodeURIComponent(path) + "/";
      
        $(".sidebar-nav li a").each(function () {
            var href = $(this).attr('href');
            
            if (path === href) {
                    $(this).addClass('active');
            }

        });
    });
</script>
</head>

